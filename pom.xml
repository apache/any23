                    < TODO restore them once ANY23-98 is fixed >
                    <exclude>**/application/nquads/test1.nq</exclude>
                    <exclude>**/application/nquads/test2.nq</exclude>
                    <exclude>**/application/rdfn3/test1</exclude>
